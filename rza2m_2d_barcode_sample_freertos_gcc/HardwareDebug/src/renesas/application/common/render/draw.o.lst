 120:../src/renesas/application/common/render/draw.c **** void DrawPoint( vdc_layer_id_t layer_id, int32_t x, int32_t y , uint8_t color )
 121:../src/renesas/application/common/render/draw.c **** {
 122:../src/renesas/application/common/render/draw.c ****     uint8_t * vram;
 123:../src/renesas/application/common/render/draw.c ****     uint8_t   data;
 124:../src/renesas/application/common/render/draw.c ****     uint32_t  width;
 125:../src/renesas/application/common/render/draw.c ****     uint32_t  height;
 126:../src/renesas/application/common/render/draw.c ****     uint32_t  stride;
 127:../src/renesas/application/common/render/draw.c ****     draw_layer_t draw_layer;
 128:../src/renesas/application/common/render/draw.c **** 
 129:../src/renesas/application/common/render/draw.c ****     draw_layer = DrawLayerID(layer_id);
 130:../src/renesas/application/common/render/draw.c **** 
 131:../src/renesas/application/common/render/draw.c ****     /* buffer get */
 132:../src/renesas/application/common/render/draw.c ****     vram = ((buffer_info[draw_layer].buff_select == 0u) ? buffer_info[draw_layer].buff_addr1
 133:../src/renesas/application/common/render/draw.c ****             : buffer_info[draw_layer].buff_addr2 );
 134:../src/renesas/application/common/render/draw.c ****     width  = buffer_info[draw_layer].window_width;
 135:../src/renesas/application/common/render/draw.c ****     height = buffer_info[draw_layer].window_height;
 136:../src/renesas/application/common/render/draw.c ****     stride = buffer_info[draw_layer].buff_stride;
 137:../src/renesas/application/common/render/draw.c **** 
 138:../src/renesas/application/common/render/draw.c ****     /* area check */
 139:../src/renesas/application/common/render/draw.c ****     if( (x < 0) || ((int)width <= x) || (y < 0) || ((int)height <= y) )
 140:../src/renesas/application/common/render/draw.c ****     {
 141:../src/renesas/application/common/render/draw.c ****         return;
 142:../src/renesas/application/common/render/draw.c ****     }
 143:../src/renesas/application/common/render/draw.c **** 
 144:../src/renesas/application/common/render/draw.c ****     /* draw */
 145:../src/renesas/application/common/render/draw.c ****     data = vram[ (x/2) + ((int)stride * y)];
 146:../src/renesas/application/common/render/draw.c **** 
 147:../src/renesas/application/common/render/draw.c ****     if( (x & 1) == 0 )
 148:../src/renesas/application/common/render/draw.c ****     {
 149:../src/renesas/application/common/render/draw.c ****         data &= 0x0F;
 150:../src/renesas/application/common/render/draw.c **** 
 151:../src/renesas/application/common/render/draw.c ****         /* set color */
 152:../src/renesas/application/common/render/draw.c ****         data |= (uint8_t)(color << 4);
 153:../src/renesas/application/common/render/draw.c ****     }
 154:../src/renesas/application/common/render/draw.c ****     else
 155:../src/renesas/application/common/render/draw.c ****     {
 156:../src/renesas/application/common/render/draw.c ****         data &= 0xF0;
 157:../src/renesas/application/common/render/draw.c ****         data |= (color & 0x0F);
 158:../src/renesas/application/common/render/draw.c ****     }
 159:../src/renesas/application/common/render/draw.c **** 
 160:../src/renesas/application/common/render/draw.c ****     /* write VRAM */
 161:../src/renesas/application/common/render/draw.c ****     vram[ (x/2) + ((int)stride * y)] = data;
 162:../src/renesas/application/common/render/draw.c **** }   /* End of function DrawPoint() */
